extends ../layout

block content
    .w3-modal(style='display:block;')
        .w3-modal-content.w3-card-4
          header.w3-container.w3-blue.w3-card-4.w3-center
            h3 Error Upload Resource 
          .w3-container.w3-margin.w3-padding.w3-section
            h3 There was an error uploading the resource! Try again later.
            a.w3-btn.w3-right.w3-blue(onclick="window.location.assign('/resources')" style="margin-left:10px; margin-top:10px") CANCEL
